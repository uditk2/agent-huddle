{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "agent-huddle-signaling",
  "main": "src/index.js",
  "compatibility_date": "2026-02-22",
  "routes": [
    {
      "pattern": "agenthuddle.synergiqai.com",
      "custom_domain": true
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "SIGNALING_ROOM",
        "class_name": "SignalingRoom"
      },
      {
        "name": "PAIR_KEY_STORE",
        "class_name": "PairKeyStore"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["SignalingRoom"]
    },
    {
      "tag": "v2",
      "new_sqlite_classes": ["PairKeyStore"]
    }
  ],
  "vars": {
    "ALLOWED_ORIGINS": "*",
    "SIGNALING_SESSION_TTL_SEC": "3600",
    "PAIR_KEY_TTL_SEC": "600",
    "PAIR_KEY_CONNECT_TTL_SEC": "600",
    "PAIR_KEY_MAX_REDEEMS": "6",
    "TURN_DEFAULT_TTL_SEC": "600",
    "TURN_AUTO_ON_SESSION": "true",
    "GOOGLE_OAUTH_ALLOWED_DOMAINS": "",
    "GOOGLE_OAUTH_REDIRECT_URI": ""
  }
}
